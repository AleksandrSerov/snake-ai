{
	"name": "ohmystart",
	"version": "1.0.0",
	"homepage": "https://aleksandrserov.github.io/spot",
	"description": "East start react project",
	"scripts": {
		"start": "tsc --noEmit --watch & parcel ./src/index.html --port 8080 --open --watch-for-stdin",
		"test": "npm run test:unit && npm run test:e2e",
		"test:unit": "jest",
		"test:e2e": "start-server-and-test start :8080 \"cypress run\"",
		"test:e2e:open": "start-server-and-test start :8080 \"cypress open\"",
		"lint": "npm run lint:css && npm run lint:scripts",
		"lint:css": "stylelint ./src/**/*.css",
		"lint:scripts": "eslint \"**/*.{js,jsx,ts,tsx}\" --ext .js,.jsx,.ts,.tsx",
		"format": "prettier-eslint --write \"./{src,cypress}/**/*.{ts,tsx,css}\"",
		"build": "rm -rf dist && parcel build --no-source-maps --public-url . ./src/index.html",
		"deploy": "gh-pages -d dist",
		"postinstall": "npx simple-git-hooks"
	},
	"author": "Aleksandr Serov",
	"license": "ISC",
	"dependencies": {
		"@inlet/react-pixi": "^6.7.0",
		"@pixi/utils": "^6.3.1",
		"@types/node": "^17.0.31",
		"@types/three": "^0.139.0",
		"classnames": "^2.3.1",
		"lodash": "^4.17.21",
		"lodash.matches": "^4.6.0",
		"parcel": "^2.2.1",
		"pixi.js": "^6.3.1",
		"react": "17.0.0",
		"react-dom": "17.0.0",
		"sharp": "0.29.1",
		"stats.js": "^0.17.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.2.3",
		"@commitlint/config-conventional": "^16.2.1",
		"@cypress/webpack-preprocessor": "^5.9.1",
		"@sserov/eslint-plugin-dirnames": "^1.0.0",
		"@sserov/presets-lint": "^0.0.9",
		"@testing-library/cypress": "^8.0.1",
		"@types/jest": "^27.4.1",
		"@types/lodash": "^4.14.182",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@types/stats.js": "^0.17.0",
		"@typescript-eslint/eslint-plugin": "^4.32.0",
		"@typescript-eslint/parser": "^4.32.0",
		"autoprefixer": "^10.4.4",
		"cypress": "^9.5.3",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-typescript": "^2.7.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-react": "^7.26.1",
		"eslint-plugin-react-hooks": "^4.4.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"eslint-plugin-unicorn": "^40.0.0",
		"gh-pages": "^3.2.3",
		"jest": "^27.2.4",
		"lint-staged": "10.2.13",
		"parcel-transformer-ts-css-modules": "^1.0.14",
		"postcss": "^8.3.8",
		"postcss-custom-properties": "^12.1.4",
		"postcss-import": "^14.0.2",
		"postcss-modules": "^4.2.2",
		"postcss-nested": "^5.0.6",
		"postcss-url": "^10.1.3",
		"prettier": "2.5.1",
		"prettier-eslint": "^13.0.0",
		"prettier-eslint-cli": "^5.0.1",
		"simple-git-hooks": "^2.6.1",
		"start-server-and-test": "^1.14.0",
		"stylelint": "^14.3.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-standard": "^24.0.0",
		"stylelint-prettier": "^1.2.0",
		"ts-jest": "^27.0.5",
		"ts-loader": "^9.2.6",
		"typescript": "4.4.3",
		"typescript-plugin-css-modules": "^3.4.0",
		"webpack": "^5.65.0"
	},
	"engines": {
		"node": "16.x"
	},
	"browserslist": {
		"production": [
			"last 2 versions",
			"ie >= 11",
			"Android >= 5",
			"iOS >= 9"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"jest": {
		"preset": "ts-jest",
		"testMatch": [
			"**/*.test.ts?(x)"
		]
	},
	"prettier": "./node_modules/@sserov/presets-lint/prettier.js",
	"eslintConfig": {
		"extends": "./node_modules/@sserov/presets-lint/eslint.js"
	},
	"stylelint": {
		"extends": "./node_modules/@sserov/presets-lint/stylelint.js"
	},
	"commitlint": {
		"extends": [
			"./node_modules/@sserov/presets-lint/commitlint.js"
		]
	},
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx}": [
			"prettier-eslint --write",
			"eslint"
		],
		"**/*.css": [
			"prettier-eslint --write",
			"stylelint"
		]
	},
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged",
		"commit-msg": "npx commitlint -e"
	}
}
